  "scripts": {
    "clean": "rm -rf ./build && rm -rf ./dist",
    "server": "node --use_strict src/server/main.js",
    "start": "export NODE_ENV=development && ./node_modules/.bin/webpack-dev-server --config=./webpack.develop.config.js --progress --inline --hot",
    "karma": "karma start",
    "lint": "eslint -c ./.eslintrc ./src/**/*.js ./test/**/**/*.js",
    "test": "karma start --single-run",
    "docs": "esdoc -c ./esdoc.config.json",
    "dev": "export NODE_ENV=development && ./node_modules/.bin/webpack --config=./webpack.develop.config.js --watch -d",
    "precompile": "npm run lint",
    "compile": "export NODE_ENV=development && ./node_modules/.bin/webpack --config=./webpack.develop.config.js --display-error-details --progress -d",
    "prebuild": "npm run validate-webpack && npm run lint && npm test && esdoc -c ./esdoc.config.json && npm run clean",
    "build": "export NODE_ENV=production && ./node_modules/.bin/parallel-webpack --config=./webpack.master.config.js --display-error-details --progress",
    "postbuild": "npm run metrics",
    "babel": "babel --optional runtime ./src/client/com.e750 --out-dir ./build/babel",
    "copy": "cp dist/*.js ~/Sync/Clients/750Group/e750.laravel/e750/core/resources/assets/js",
    "metrics": "node --use_strict ./node_modules/.bin/es6-plato -r -t 'e750.js MicroFramework' -d ./reports/metrics ./src/client"
  },
