{
  "name": "e750-Webflow",
  "version": "0.0.0",
  "description": "750Commerce",
  "main": "lib/cli/main.js",
  "dependencies": {
    "babel": "^5.5.5",
    "core-js": "^0.9.8",
    "rsvp": "^3.0.18",
    "templeton": "^2.1.1",
    "xhttp": "^0.0.6",
    "cookie-parser": "^1.3.5",
    "body-parser": "^1.12.4",
    "express": "^4.12.4",
    "pem": "^1.7.2",
    "request-promise": "^0.4.2"
  },
  "devDependencies": {
    "babel-runtime": "^5.5.5",
    "plato": "^1.5.0",
    "babel-eslint": "^3.1.26",
    "babel-istanbul": "^0.2.10",
    "babelify": "^6.1.0",
    "browserify": "^10.1.3",
    "eslint": "^0.21.0",
    "isparta": "^3.0.3",
    "karma": "^0.13.3",
    "karma-babel-preprocessor": "^5.2.1",
    "karma-browserify": "^4.2.1",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "karma-firefox-launcher": "^0.1.6",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-tap": "^1.0.3",
    "karma-tape-reporter": "^1.0.3",
    "minifyify": "^7.0.3",
    "phantomjs": "^1.9.17",
    "tape": "^4.0.1"
  },
  "scripts": {
    "lint": "eslint -c ./.eslintrc ./lib/**/*.js",
    "test": "karma start",
    "coverage": "babel-node node_modules/.bin/isparta cover --report text --report html ./lib/client/main.js --dir ./reports/coverage",
    "compile": "browserify ./lib/client/main.js -t [babelify --sourceMapRelative ./lib] --outfile acme-vineyards-webflow/js/e750.js",
    "build": "browserify -d ./lib/client/main.js -t [babelify --sourceMapRelative ./lib] -p [minifyify --map /js/e750.js.map --output ./acme-vineyards-webflow/js/e750.js.map] --outfile acme-vineyards-webflow/js/e750.min.js",
    "start": "babel-node ./lib/server/main.js",
    "metrics:compile_source": "./node_modules/.bin/babel --optional runtime lib/ --out-dir ./build/metrics/lib",
    "metrics": "npm run metrics:compile_source && ./node_modules/.bin/plato -r  -l .eslintrc -d ./reports/metrics ./build/metrics/lib"

  },
  "author": "Alessandro Di Candia",
  "license": "ISCs"
}
