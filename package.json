  "scripts": {
    "clean": "rm -rf ./build && rm -rf ./dist",
    "server": "node --use_strict src/server/main.js",
    "start": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config=./webpack.develop.config.js --progress --inline --hot",
    "karma": "npm run test",
    "lint": "eslint -c ./.eslintrc ./src/client/**/* ./test/**/*",
    "test": "NODE_ENV=test karma start --single-run",
    "test:watch": "npm run test -- --no-single-run",
    "docs": "esdoc -c ./esdoc.config.json",
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack --config=./webpack.develop.config.js --watch -d",
    "precompile": "npm run lint",
    "compile": "export NODE_ENV=development && ./node_modules/.bin/webpack --config=./webpack.develop.config.js --display-error-details --progress -d",
    "prebuild": "NODE_ENV=production npm run clean && karma start --single-run",
    "build": "NODE_ENV=production ./node_modules/.bin/parallel-webpack --config=./webpack.master.config.js --display-error-details --progress",
    "postbuild": "esdoc -c ./esdoc.config.json && npm run metrics",
    "babel": "babel --optional runtime ./src/client/com.e750 --out-dir ./build/babel",
    "copy": "cp dist/*.js ~/Sync/Clients/750Group/e750.laravel/e750/core/resources/assets/js",
    "metrics": "node --use_strict ./node_modules/.bin/es6-plato -r -t 'e750.js MicroFramework' -d ./reports/metrics ./src/client"
  },
