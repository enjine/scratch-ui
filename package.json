{
  "name": "e750-Micro",
  "version": "0.0.0",
  "description": "750Commerce Micro-framework",
  "main": "lib/client/main.js",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "body-parser": ">=1.12.4",
    "cookie-parser": ">=1.3.5",
    "es6-promise": "^3.0.2",
    "exports-loader": "^0.6.3",
    "express": ">=4.12.4",
    "imports-loader": "^0.6.5",
    "pem": ">=1.7.2",
    "request-promise": ">=0.4.2",
    "templeton": ">=2.1.1",
    "whatwg-fetch": ">=0.11.0"
  },
  "devDependencies": {
    "babel-core": ">=6.0.0",
    "babel-eslint": ">=5.0.0-beta10",
    "babel-istanbul": ">=0.2.10",
    "babel-loader": ">=5.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": ">=6.1.0",
    "browserify": ">=11.2.0",
    "browserify-istanbul": ">=0.2.1",
    "chai": ">=3.2.0",
    "chai-as-promised": ">=5.1.0",
    "esdoc-es7-plugin": ">=0.0.3",
    "eslint": ">=0.21.0",
    "isparta": ">=3.0.3",
    "karma": ">=0.13.3",
    "karma-browserify": ">=4.4.0",
    "karma-chai": ">=0.1.0",
    "karma-chai-as-promised": ">=0.1.2",
    "karma-chrome-launcher": ">=0.2.0",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "karma-firefox-launcher": ">=0.1.6",
    "karma-junit-reporter": ">=0.3.3",
    "karma-mocha": ">=0.2.0",
    "karma-mocha-reporter": ">=1.0.4",
    "karma-notify-reporter": ">=0.1.1",
    "karma-phantomjs-launcher": ">=0.2.0",
    "karma-safari-launcher": ">=0.1.1",
    "karma-sinon-chai": ">=1.0.0",
    "karma-sourcemap-loader": ">=0.3.5",
    "minifyify": ">=7.0.3",
    "mocha": ">=2.2.5",
    "phantomjs-prebuilt": ">=2.1.3",
    "plato": ">=1.5.0",
    "react": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sinon": ">=1.15.4",
    "watchify": "^3.7.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": "5.5.0"
  },
  "scripts": {
    "lint": "eslint -c ./.eslintrc ./src/**/*.js ./test/**/**/*.js",
    "test": "karma start --single-run",
    "docs": "esdoc -c ./esdoc.json",
    "precompile": "npm run lint",
    "compile": "browserify ./src/client/main.js -t [babelify --sourceMapRelative ./src] --outfile dist/e750.js",
    "prebuild": "npm run lint && npm test && esdoc -c ./esdoc.json",
    "build": "browserify -d ./src/client/main.js -t [babelify --sourceMapRelative ./src] -p [minifyify --map /js/e750.js.map --output dist/e750.js.map] --outfile dist/e750.min.js",
    "postbuild": "npm run copy && npm run metrics",
    "start": "babel-node ./src/server/main.js",
    "copy": "cp dist/*.js ~/Sync/Clients/750Group/e750.laravel/e750/core/resources/assets/js && cp dist/e750.js.map ~/Sync/Clients/750Group/e750.laravel/e750/core/resources/assets/js",
    "premetrics": "babel --optional runtime src/ --out-dir ./build/metrics/src",
    "metrics:compile_source": "./node_modules/.bin/babel --optional runtime ./src/ --out-dir ./build/metrics/src",
    "metrics": "npm run metrics:compile_source && ./node_modules/.bin/plato -r -t 'e750.js MicroFramework' -l .jshintrc -d ./reports/metrics ./build/metrics/src"
  },
  "author": "Alessandro Di Candia",
  "license": "BSD"
}
