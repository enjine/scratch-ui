  "scripts": {
    "lint": "eslint -c ./.eslintrc ./src/**/*.js ./test/**/**/*.js",
    "test": "karma start --single-run",
    "docs": "esdoc -c ./esdoc.json",
    "precompile": "npm run lint",
    "compile": "node_modules/webpack/bin/webpack.js --display-error-details --progress -p",
    "prebuild": "npm run lint && npm test && esdoc -c ./esdoc.json",
    "build": "node_modules/webpack/bin/webpack.js --display-error-details --progress -p",
    "postbuild": "npm run copy && npm run metrics",
    "start": "babel-node ./src/server/main.js",
    "copy": "cp dist/*.js ~/Sync/Clients/750Group/e750.laravel/e750/core/resources/assets/js && cp dist/e750.js.map ~/Sync/Clients/750Group/e750.laravel/e750/core/resources/assets/js",
    "premetrics": "babel --optional runtime src/client/com.e750 --out-dir ./build/metrics/src",
    "metrics": "./node_modules/.bin/plato -r -t 'e750.js MicroFramework' -e .eslintrc -d ./reports/metrics ./build/metrics/src"
  },
